{
  "dashboard": {
    "title": "Coffee Tracker Overview",
    "uid": "coffee-tracker-overview",
    "tags": ["coffee", "caffeine", "heartrate"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Total Caffeine (mg)",
        "type": "stat",
        "targets": [
          {
            "expr": "coffee_total_caffeine_mg",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Latest Heart Rate (BPM)",
        "type": "stat",
        "targets": [
          {
            "expr": "heartrate_latest_bpm",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Coffee Logs Today",
        "type": "stat",
        "targets": [
          {
            "expr": "coffee_logs_today_count",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Heart Rate Logs Today",
        "type": "stat",
        "targets": [
          {
            "expr": "heartrate_logs_today_count",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Response Time (ms)",
        "type": "graph",
        "targets": [
          {
            "expr": "http_request_duration_seconds_sum / http_request_duration_seconds_count * 1000",
            "refId": "A",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ]
  },
  "overwrite": true
}
